    "start:config": "cd ./mocks && webpack-dev-server --config ./webpack.register.config.dev.js --port 4007",
    "start:common-deps": "cd ./mocks && webpack-dev-server --config ./webpack.common-deps.config.dev.js --port 4005",
    "build:all": "yarn build:config && yarn build:common-deps && yarn build",
    "build:config": "cd ./mocks && webpack --config ./mocks/webpack.register.config.js -p",
    "build:common-deps": "cd ./mocks && webpack --config ./webpack.common-deps.config.js -p",
    "lint": "eslint \"**/*.{js,ts,tsx}\" ",
    "install:mock": "cd ./mocks && yarn install",
    "install:all": "yarn install && yarn install:mock"
  },
